# Uncomment the next line to define a global platform for your project
platform :ios, ’10.0’

target 'My first app' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

pod ‘Charts’
pod ‘SwiftLint’
pod ‘RealmSwift’

  target 'My first appTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'My first appUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
